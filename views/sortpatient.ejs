<table border="1" background-color="red">
    <thead>
        <tr>
            <th>Patient ID</th>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        <% sortedpatient.forEach(row => { %>
        <tr>
            <td>
                <%=row.patient_id %>
            </td>
            <td>
                <%=row.first_name %>
            </td>
            <td>
                <%=row.last_name %>
            </td>
        </tr>
        <% }); %>
    </tbody>
</table>